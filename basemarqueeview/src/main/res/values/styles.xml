<?xml version="1.0" encoding="utf-8"?>
<resources>
  <!--base marquee-->
  <declare-styleable name="BaseMarqueeViewStyle">
    <attr format="integer|reference" name="interval" />
    <attr format="integer|reference" name="duration" />
    <attr name="direction">
      <enum name="bottom_to_top" value="0" />
      <enum name="top_to_bottom" value="1" />
      <enum name="right_to_left" value="2" />
      <enum name="left_to_right" value="3" />
    </attr>
  </declare-styleable>
</resources>